#include <stdio.h>
int main(){
int arr[] = {5, 1, 2, 4, 3, 6, 9, 8, 7};

int k, n, i, j, temp; 
scanf("%d", &k);

n = sizeof(arr)/sizeof(arr[0]);

if(k <= n && k >= 0){  

for(i = 0; i < k; i++){
    for(j = i+1; j < n; j++){

        if(arr[i] > arr[j]){
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
        }
    }
            
}

for(i = k; i < n - 1; i++){

    for(j = i+1; j < n; j++){

        if(arr[i] < arr[j]){
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
        }
    }
}



for(i = 0; i < n; i++){
    printf("%d ", arr[i]);
}
}
return 0;
}
